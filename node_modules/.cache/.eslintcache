umber, categorySlide } = context;\n  const [current, setCurrent] = useState(slideStartNumber)\n\n  useEffect(() => {\n    const slideLength = categorySlide.length\n    const setTimer = setTimeout(() => {\n      current < slideLength - 1 ? setCurrent(current + 1) : setCurrent(0);\n    }, 7000)\n    setTimer();\n    return clearTimeout(setTimer)\n  }, [current])\n\n  let imageSlide = `http://localhost:5000/${categorySlide[current].images}`;\n  let titleSlide = categorySlide[current].title;\n  let descriptionSlide = categorySlide[current].description;\n\n  return (\n    <>\n\n      <FrameImage>\n        <BoxDisplay image={imageSlide} />\n        <Children>\n          <Title>{titleSlide}</Title>\n          {descriptionSlide}\n        </Children>\n      </FrameImage>\n\n      <DotWrapper>\n        {\n          categorySlide.map((_, i) => (\n            <Dot\n              key={i}\n              activeDot={current === i}\n              onClick={() => setCurrent(i)}\n            />\n          ))\n        }\n      </DotWrapper>\n    </>\n  )\n\n};\n\nexport default PortfolioRendered;\n","/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/pages/ErrorPage.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/Footer.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/Navbar.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/pages/Blog.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/pages/BlogDetail.jsx",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/context.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/About.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/Skills.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/NavMenu.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/ModalPortfolio.jsx",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/pages/BlogCard.jsx",[],[],{"ruleId":"99","severity"