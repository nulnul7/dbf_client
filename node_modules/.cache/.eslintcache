f/dbf_client/src/pages/PortfolioRendered.js",["93"],[],"import React, { useContext, useState, useEffect } from 'react';\nimport styled from 'styled-components';\nimport gambar from '../assets/defaultImages.jpeg'\nimport { Context } from '../context'\n\n\nconst FrameImage = styled.div`\n      width: 80vw;\n      height: 67vh;\n      margin: 75px auto 0 auto;\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n`\nconst DotWrapper = styled.div`\n    display: flex;\n    margin: 0 auto;\n    padding-left: 30px;\n    width: 50%;\n    justify-content: center;\n`\nconst Dot = styled.div`\n    width: 18px;\n    height: 6px;\n    border-radius: 15%;\n    background-color: ${(props) => props.activeDot ? 'greenyellow' : 'darkgray'};\n    z-index: 10;\n    margin-top: 20px;\n    margin-right: 10px;\n    transition: all 500ms ease-in-out;\n    cursor: pointer;\n`\nconst Title = styled.h4`\n    text-transform: uppercase;\n    margin-bottom: 10px;\n    margin-top: 30px;\n    font-size: 1.1em;\n    color: black;\n`\nconst BoxDisplay = styled.img.attrs(props => ({\n  src: props.image || gambar\n}))`\n    height: 80%;\n    background-color: green;\n    background-position: center;\n    background-repeat: no-repeat;\n    background-size: cover;\n    display: block;\n`\n\n// BoxDisplay.defaultProps = {\n//   src: gambar,\n// }\n\nconst Children = styled.div`\n    width: 70%;\n    font-size: 0.8em;\n    font-weight: 400;\n    color: darkgray;\n`\n\nconst PortfolioRendered = ({\n  activeDot,\n  textData,\n  ...props\n}) => {\n\n\n  const context = useContext(Context)\n  const { slideStartNumber, categorySlide } = context;\n  const [current, setCurrent] = useState(slideStartNumber)\n\n  useEffect(() => {\n    const slideLength = categorySlide.length\n    const setTimer = setTimeout(() => {\n      current < slideLength - 1 ? setCurrent(current + 1) : setCurrent(0);\n    }, 7000)\n    setTimer();\n    return clearTimeout(setTimer)\n  }, [current])\n\n  let imageSlide = `http://localhost:5000/${categorySlide[current].images}`;\n  let titleSlide = categorySlide[current].title;\n  let descriptionSlide = categorySlide[current].description;\n\n  return (\n    <>\n\n      <FrameImage>\n        <BoxDisplay image={imageSlide} />\n        <Children>\n          <Title>{titleSlide}</Title>\n          {descriptionSlide}\n        </Children>\n      </FrameImage>\n\n      <DotWrapper>\n        {\n          categorySlide.map((_, i) => (\n            <Dot\n              key={i}\n              activeDot={current === i}\n              onClick={() => setCurrent(i)}\n            />\n          ))\n        }\n      </DotWrapper>\n    </>\n  )\n\n};\n\nexport default PortfolioRendered;\n","/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/pages/ErrorPage.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/Footer.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/Navbar.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/pages/Blog.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/pages/BlogDetail.jsx",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/context.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/About.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/Skills.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/NavMenu.js",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/components/ModalPortfolio.jsx",[],[],"/Users/erteka/Desktop/react/dbfPortf/dbf_client/src/pages/BlogCard.jsx",[],[],{"ruleId":"94","severity":1,"message":"95","line":80,"column":6,"nodeType":"96","endLine":80,"endColumn":15,"suggestions":"97"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'categorySlide.length'. Either include it or remove the dependency array.","ArrayExpression",["98"],{"desc":"99","fix":"100"},"Update the dependencies array to be: [categorySlide.length, current]"